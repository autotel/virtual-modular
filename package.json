    "fifo": "^2.3.0",
    "jazz-midi": "^1.6.5",
    "jazz-midi-electron": "^1.0.4",
    "jzz": "^0.5.9",
    "nodemon": "^1.19.4",
    "onhandlers": "^1.1.1",
    "osc": "^2.3.1",
    "pegjs": "^0.10.0",
    "prompt": "^1.0.0",
